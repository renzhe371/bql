<script>
var routeBox=$(".routeBox")
var routesite = routeBox.find("li");
var air=$(".air");
air.css("top",routeBox.height()-air.height()/2-2);
var routeArrow=$(".routeArrow");
//已经完成的站点数目	var  siteNum=6;
var Deadline=[
	{
		yearT:2015,
		monthT:5,
		dateT:28
	},
	{
		yearT:2015,
		monthT:6,
		dateT:25
	},
	{
		yearT:2015,
		monthT:7,
		dateT:2
	},
	{
		yearT:2015,
		monthT:7,
		dateT:23
	},
	{
		yearT:2016,
		monthT:8,
		dateT:28
	},
	{
		yearT:2016,
		monthT:9,
		dateT:18
	},
	{
		yearT:2016,
		monthT:10,
		dateT:18
	},
	{
		yearT:2016,
		monthT:10,
		dateT:28
	},
	{
		yearT:2016,
		monthT:10,
		dateT:28
	},
	{
		yearT:2016,
		monthT:11,
		dateT:18
	}
];
var date= new Date();
var yearN=date.getFullYear();
var monthN=date.getMonth()+1;
var dateN=date.getDate();
var siteNum=function(){

	var lastestDay = -1;
	for(var i=0;i<10;i++){
	
	   if( (yearN>Deadline[i].yearT)||(yearN==Deadline[i].yearT&&monthN>Deadline[i].monthT)||(yearN==Deadline[i].yearT&&monthN==Deadline[i].monthT&&dateN>Deadline[i].dateT)){
	   
			lastestDay = i+1;   
	   }
	}
	return lastestDay;
}();

$(window).load(function(){

	routeBox.animate({top:0},1200)
})
  
var i=0,j=0;
var timerR,timeOut;
var arrow = $(".direct_arrow span");
   
function   showTimeO(){

	function showSite(){
  
		routesite.eq(i).fadeIn(); 
		i++;
		if(i>routesite.length){
		  
			arrow.fadeIn();
			routeArrow.fadeIn();
			i=routesite.length;
			clearTimeout(timeOut);
			clearInterval(timerR); 
			air.show();
			animaArrow();
		}
	}
	timerR=setInterval(showSite,500);
}
timeOut= setTimeout("showTimeO()",1200);
	 
function  animaArrow(){
	 
	if(siteNum<5){
		function earStage(){
			
			var k=1,timeraW;
			function aW(){
				
				air.animate({
				
					left:parseFloat(routesite.eq(k).find("dt").offset().left)-parseFloat(routeBox.offset().left)-air.width()/2,
				},500,function(){
			
					routesite.eq(k-1).addClass('over');
					$(".over").find("img").animate({left:0},200);
					if(yearN==Deadline[k].yearT&&monthN==Deadline[k].monthT&&dateN==Deadline[k].dateT){
					
						routesite.eq(k).find("dt").css("background-position","right bottom");
						air.css("opacity","0.2");
					}
					k++;
				})
				if(k>=siteNum){
					
					clearInterval(timeraW);
				}
			}		 
			timeraW=setInterval(aW,1000);
		}
		earStage()
	}
	else if(siteNum==5){
		
		function midStage(){
				 
			var k=1,timeraW;
			function aW(){
				   
				if(k<5){
				
					air.animate({
					
						left:parseFloat(routesite.eq(k).find("dt").offset().left)-parseFloat(routeBox.offset().left)-air.width()/2,
					},500,function(){
				
						routesite.eq(k-1).addClass('over');
						$(".over").find("img").animate({left:0},200);
						k++;
					})
				}
				else if(k==5){
						 
					air.animate({
					
						left:routeBox.width()-air.width()/2-2,
					},500,function(){
					
						air.css({"transform":"rotate(-90deg)","-webkit-transform":"rotate(-90deg)","-moz-transform":"rotate(-90deg)","-ms-transform":"rotate(-90deg)","top":routeBox.height()-air.width()*2});
					}).animate({
					
						top:parseFloat(routesite.eq(k-1).find("dt").offset().top)-parseFloat(routeBox.offset().top)-air.width()*3,
					},300,function(){
						
						routesite.eq(k-1).addClass('over');
						routesite.eq(k-1).find("img").css({"top":"100%","left":"0"});
						$(".over").find("img").animate({top:0},200);
						if(yearN==Deadline[k].yearT&&monthN==Deadline[k].monthT&&dateN==Deadline[k].dateT){
						
							routesite.eq(k).find("dt").css("background-position","right bottom");
							air.css("opacity","0.2");
						}
						k++;
					})
				}
				// console.log(k);
				else{
				
					clearInterval(timeraW);
				}
			}//aw函数
			timeraW=setInterval(aW,1000);
		}
		midStage()
		
	}
	else{
		
		function laterStage(){
		
			var k=1,timeraW;
			function aW(){
			
				if(k<5){
				
					air.animate({
					
						left:parseFloat(routesite.eq(k).find("dt").offset().left)-parseFloat(routeBox.offset().left)-air.width()/2,
					},500,function(){
				
						routesite.eq(k-1).addClass('over');
						$(".over").find("img").animate({left:0},200);
						k++;
					})
				}
				else if(k==5){
					 
					air.animate({
					
						left:routeBox.width()-air.width()/2-2,
					},500,function(){
					
						air.css({"transform":"rotate(-90deg)","-webkit-transform":"rotate(-90deg)","-moz-transform":"rotate(-90deg)",
						"-ms-transform":"rotate(-90deg)","top":routeBox.height()-air.width()*2});
					}).animate({
					
						top:parseFloat(routesite.eq(k).find("dt").offset().top)-parseFloat(routeBox.offset().top)-air.height()/2,
					},300,function(){
					
						routesite.eq(k-1).addClass('over');
						$(".over").find("img").animate({
							 left:0
						 },200)
						 k++;
					 })
				}   
				  
				else if(k>5 && k<=siteNum){ 
				
					air.css({top:-air.height()/2,"transform":"rotate(-180deg)","-webkit-transform":"rotate(-180deg)","-moz-transform":"rotate(-180deg)","-ms-transform":"rotate(-180deg)"}).animate({
							  
						left:parseFloat(routesite.eq(k-1).offset().left)-parseFloat(routeBox.offset().left)-air.width()*2-air.width()/2
					},200,function(){
					
						routesite.eq(k-1).addClass('over');
						routesite.eq(k-1).find("img").css({"left":"100%","top":"0"})
						$(".over").find("img").animate({left:0},200)
						
							if(Deadline[k]){
								
								if(yearN==Deadline[k].yearT&&monthN==Deadline[k].monthT&&dateN==Deadline[k].dateT){
								   
									routesite.eq(k).find("dt").css("background-position","right bottom");
									air.css("opacity","0.2");
								}
							}
						//  console.log(k)
						k++;
					})
				} 
				// console.log(k);
				else{
				
					clearInterval(timeraW);
				   }
			}//aw函数
			timeraW=setInterval(aW,1000);
		}
		laterStage()
	}
}
	 
routesite.each(function(){
 
	var _this=$(this);
	 
	_this.hover(function(){
	
		if(_this.attr("class").search("over")>-1){
		
		   _this.find("dd").css("display","block");
		} 	
			
	},function(){
	 
		_this.find("dd").css("display","none");
	}) 
})
