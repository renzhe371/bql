<script>
var timeObj=$(".time li")
var dashijiUL=$(".dashi_list_pic ul")
var dashijiOL=$(".dashi_list_pic ol")
var dashiPic=$(".dashi_list_pic");
var dashiPicTop=dashiPic.offset().top;
dashijiUL.find("li").each(function(index){
  
	$(this).hover(function(){
	  
		dashijiOL.find("li.current").removeClass("current");
		dashijiOL.find("li").eq(index).addClass("current");
	});
});

dashijiOL.last().find('li').first().addClass('current');
timeObj.last().addClass("there");
timeObj.each(function(index){

	$(this).click(function(){
	
		timeObj.removeClass("there");
		$(this).addClass("there");
		$(".dashi_list_pic ul.display").removeClass("display");
		dashijiUL.eq(index).addClass("display");
		$(".dashi_list_pic ol.display").removeClass("display");
		dashijiOL.eq(index).addClass("display");
		dashijiOL.find("li").removeClass("current");
		$(".dashi_list_pic ol.display").find("li").eq(0).addClass("current");
		
		$(document).scroll(function(){
		 
			if($(document).scrollTop()>dashiPicTop){
			
				dashijiOL.css({'position':'fixed','top':120})  
				
				if(dashiPicTop+dashiPic.height()<$(document).scrollTop()+200){
				
					dashijiOL.css({'position':'absolute','top':120})  
				}
			}else{
			   
				dashiPic.find('ol').css({'position':'relative','top':0})     
			}
		}) 
	});
});

dashijiOL.find('img').each(function(){

	if($(this).attr('src')==''){
  
		$(this).remove();
	}
})
  
dashijiUL.find("li").each(function(){

	$(this).hover(function(){
  
		$(this).find("span").not(".dashiTime").css("color","#008f9b")
	})
	$(this).mouseout(function(){
   
		$(this).find("span").not(".dashiTime").css("color","#666")
	})
});
</script> 